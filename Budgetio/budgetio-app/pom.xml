			<groupId>me.figo</groupId>
			<artifactId>sdk</artifactId>
			<version>${figo.sdk.version}</version>
		</dependency>
		<dependency>
			<groupId>com.google.code.gson</groupId>
			<artifactId>gson</artifactId>		
			<version>${google.gson.version}</version>
